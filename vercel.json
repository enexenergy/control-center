{
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "includeFiles": "templates/**,static/**,scripts/**,*.json",
            "maxDuration": 60
        }
    }
}